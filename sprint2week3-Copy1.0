   "source": [
    "from sklearn.preprocessing import normalize"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "60add7ca",
   "metadata": {},
   "outputs": [],
   "source": [
    "X = df[['Age', 'DistanceFromHome', 'DailyRate']]\n",
    "X = normalize(X)\n",
    "y = df['Attrition']\n",
    "X_train, X_test, y_train, y_test = train_test_split(X, y, test_size=0.3, random_state=1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "0efabec1",
   "metadata": {},
